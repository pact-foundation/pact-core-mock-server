{
  "provider": {
    "name": "http-service"
  },
  "consumer": {
    "name": "Consumer"
  },
  "interactions": [
    {
      "description": "a request with a last modified header",
      "request": {
        "method": "GET",
        "path": "/myapp/test"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json",
          "Last-Modified": "Sun, 12 Mar 2023 01:21:35 GMT"
        },
        "body": {},
        "matchingRules": {
          "$.headers.Last-Modified": {
            "match": "regex",
            "regex": "^[A-Za-z]{3},\\s\\d{2}\\s[A-Za-z]{3}\\s\\d{4}\\s\\d{2}:\\d{2}:\\d{2}\\sGMT$"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}
